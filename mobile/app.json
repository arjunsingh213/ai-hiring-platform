{
  "expo": {
    "name": "Froscel",
    "slug": "froscel-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.froscel.mobile",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera is required for AI interviews",
        "NSMicrophoneUsageDescription": "Microphone is required for AI interviews"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.froscel.mobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "https://your-backend-url.com/api"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
